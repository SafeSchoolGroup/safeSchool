<?php

namespace AppBundle\Entity;

/**
 * ProgrammeCours
 */
class ProgrammeCours
{
    /**
     * @var integer
     */
    private $id;


    /**
     * @var \DateTime
     */
    private $dateProgramme;

    /**
     * @var \DateTime
     */
    private $heureDebut;

    /**
     * @var \DateTime
     */
    private $heureFin;

    /**
     * @var string
     */
    private $jour;

    /**
     * @var \AppBundle\Entity\Enseigner
     */
    private $enseigner;

    /**
     * @var \AppBundle\Entity\Salle
     */
    private $salle;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set dateProgramme
     *
     * @param \DateTime $dateProgramme
     *
     * @return ProgrammeCours
     */
    public function setDateProgramme($dateProgramme)
    {
        $this->dateProgramme = $dateProgramme;

        return $this;
    }

    /**
     * Get dateProgramme
     *
     * @return \DateTime
     */
    public function getDateProgramme()
    {
        return $this->dateProgramme;
    }

    /**
     * Set heureDebut
     *
     * @param \DateTime $heureDebut
     *
     * @return ProgrammeCours
     */
    public function setHeureDebut($heureDebut)
    {
        $this->heureDebut = $heureDebut;

        return $this;
    }

    /**
     * Get heureDebut
     *
     * @return \DateTime
     */
    public function getHeureDebut()
    {
        return $this->heureDebut;
    }

    /**
     * Set heureFin
     *
     * @param \DateTime $heureFin
     *
     * @return ProgrammeCours
     */
    public function setHeureFin($heureFin)
    {
        $this->heureFin = $heureFin;

        return $this;
    }

    /**
     * Get heureFin
     *
     * @return \DateTime
     */
    public function getHeureFin()
    {
        return $this->heureFin;
    }

    /**
     * Set jour
     *
     * @param string $jour
     *
     * @return ProgrammeCours
     */
    public function setJour($jour)
    {
        $this->jour = $jour;

        return $this;
    }

    /**
     * Get jour
     *
     * @return string
     */
    public function getJour()
    {
        return $this->jour;
    }

    /**
     * Set enseigner
     *
     * @param \AppBundle\Entity\Enseigner $enseigner
     *
     * @return ProgrammeCours
     */
    public function setEnseigner(\AppBundle\Entity\Enseigner $enseigner = null)
    {
        $this->enseigner = $enseigner;

        return $this;
    }

    /**
     * Get enseigner
     *
     * @return \AppBundle\Entity\Enseigner
     */
    public function getEnseigner()
    {
        return $this->enseigner;
    }

    /**
     * Set salle
     *
     * @param \AppBundle\Entity\Salle $salle
     *
     * @return ProgrammeCours
     */
    public function setSalle(\AppBundle\Entity\Salle $salle = null)
    {
        $this->salle = $salle;

        return $this;
    }

    /**
     * Get salle
     *
     * @return \AppBundle\Entity\Salle
     */
    public function getSalle()
    {
        return $this->salle;
    }
}
